use SimulationDB

create table Snapshot (
snapnum smallint not null,
phkey integer not null,
id bigint not null,
x real not null,
y real not null,
z real not null,
vx real not null,
vy real not null,
vz real not null)

create table SnapshotArr (
snapnum smallint not null,
phkey integer not null,
numPart integer not null,
pos varbinary(max) not null,
vel varbinary(max) not null,
id varbinary(max) not null)

